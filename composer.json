{
    "name": "shapecode/matomo-bundle",
    "description": "This bundle provides matomo functions.",
    "keywords": [
        "matomo",
        "piwik",
        "shapecode",
        "symfony",
        "bundle"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "https://github.com/shapecode/matomo-bundle",
    "support": {
        "email": "support@shapeocode.de",
        "issues": "https://github.com/shapecode/matomo-bundle/issues",
        "source": "https://github.com/shapecode/matomo-bundle/releases",
        "wiki": "https://github.com/shapecode/matomo-bundle/wiki"
    },
    "authors": [
        {
            "name": "Nikita Loges",
            "homepage": "https://nikita-loges.de",
            "email": "develop@nikita-loges.de"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",

        "symfony/framework-bundle": "~2.8|~3.0|~4.0",
        "symfony/dependency-injection": "~2.8|~3.0|~4.0",
        "symfony/http-kernel": "~2.8|~3.0|~4.0",
        "symfony/config": "~2.8|~3.0|~4.0",
        "symfony/options-resolver": "~2.8|~3.0|~4.0",
        "symfony/twig-bundle": "~2.8|~3.0|~4.0",

        "twig/twig": "~1.34|~2.0"
    },
    "replace": {
        "shapecode/piwik-bundle": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Shapecode\\Bundle\\MatomoBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
