{
    "name": "shapecode/piwik-bundle",
    "description": "This bundle provides piwik functions.",
    "keywords": [
        "piwik",
        "shapecode",
        "symfony",
        "bundle"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "https://github.com/shapecode/piwik-bundle",
    "support": {
        "email": "support@shapeocode.de",
        "issues": "https://github.com/shapecode/piwik-bundle/issues",
        "source": "https://github.com/shapecode/piwik-bundle/releases",
        "wiki": "https://github.com/shapecode/piwik-bundle/wiki"
    },
    "authors": [
        {
            "name": "Nikita Loges",
            "homepage": "https://nikita-loges.de",
            "email": "develop@nikita-loges.de"
        }
    ],
    "require": {
        "php": "~5.5|~7.0",

        "symfony/framework-bundle": "~2.8|~3.0",
        "symfony/dependency-injection": "~2.8|~3.0",
        "symfony/http-kernel": "~2.8|~3.0",
        "symfony/config": "~2.8|~3.0",
        "symfony/options-resolver": "~2.8|~3.0",
        "symfony/twig-bundle": "~2.8|~3.0",

        "twig/twig": "~1.0|~2.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "~2.8|~3.0"
    },
    "autoload": {
        "psr-4": {
            "Shapecode\\Bundle\\PiwikBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shapecode\\Bundle\\PiwikBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-release/1.0": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
